{
  "manifest_version": 3,
  "name": "EasySwitch (Google)",
  "version": "1.0.0",
  "description": "Easily switch between multiple Google and YouTube accounts with custom nicknames.",
  "permissions": ["tabs", "storage", "activeTab"],
  "host_permissions": [
    "https://*.google.com/*",
    "https://*.youtube.com/*",
    "https://*.googleapis.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "EasySwitch (Google)",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "author": "Surya",
  "homepage_url": "https://github.com/yourusername/easyswitch-google"
}